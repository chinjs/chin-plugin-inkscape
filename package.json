{
  "name": "chin-plugin-inkscape",
  "version": "0.0.4",
  "repository": "kthjm/chin-plugin-inkscape",
  "author": "kthjm <toxictoxer@gmail.com>",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "Chin plugin that operate Inkscape(and PDFtk).",
  "keywords": [
    "chin",
    "plugin",
    "inkscape",
    "svg",
    "png",
    "pdf",
    "pdf-merge",
    "pdftk"
  ],
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "ex": "cross-env BABEL_ENV=EXAMPLE chin -c .example -r babel-register",
    "test": "cross-env BABEL_ENV=TEST nyc mocha",
    "prebuild": "yarn test",
    "build": "rollup -c",
    "coverage": "nyc report --reporter=lcov && codecov"
  },
  "dependencies": {
    "fs-extra": "^4.0.3",
    "inkscape": "^1.2.0",
    "param-case": "^2.1.1",
    "pdf-merge": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-power-assert": "^2.0.0",
    "chin": "^3.0.2",
    "codecov": "^3.0.1",
    "cross-env": "^5.1.4",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "power-assert": "^1.5.0",
    "rollup": "^0.58.2",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-prettier": "^0.4.0"
  },
  "peerDependencies": {
    "chin": "^3.0.1"
  }
}
